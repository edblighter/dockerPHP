# This file can be used to customize Beekeeper Studio settings
# It's an alternative to using the .env.beekeper file in the services/beekeper directory

# Beekeeper Studio container name
BEEKEEPER_CONTAINER_NAME=app_beekeeper

# Beekeeper Studio access port
BEEKEEPER_EXTERNAL_PORT=8080

# VNC configuration
BEEKEEPER_VNC_RESOLUTION=1280x800
BEEKEEPER_VNC_COLOR_DEPTH=24

# Container memory limits
BEEKEEPER_MEM_LIMIT=512M